<?xml version="1.0" encoding="UTF-8"?>
<vocabularies>
    <ontology name="cessda">
        <type>cessda</type>
        <api>https://vocabularies.cessda.eu</api>
        <uri>v1/suggest/Vocabulary/$vocab/version/1.0/language/en/limit/10/query/$term</uri>
    </ontology>
    <ontology name="Unit of Analysis">

        <vocabulary>covidqa</vocabulary>
        <api>https://thesauri.cessda.eu</api>
        <uri>rest/v1/search</uri>
        <parameters>
            <vocab>$vocab</vocab>
            <query>$term</query>
            <lang>$lang</lang>
        </parameters>
    </ontology>
